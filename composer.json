{
    "license": "MIT",
    "name": "zymawy/z-store",
    "homepage": "http://zymawy.com",
    "type": "laravel-bundle",
    "description": "The z-store e-commerce Framework.",
    "keywords": ["z-store", "laravel", "e-commerce", "ecommerce", "shop", "portal", "market", "store"],
    "minimum-stability": "dev",

    "authors": [
        {
            "name": "Hamza Mallam",
            "email": "zymawy@gmail.com"
        }
    ],

    "support": {
        "source": "https://github.com/zymawy/z-store",
        "issues": "https://github.com/zymawy/z-store/issues"
    },

    "require": {
        "php": "^7.1.3",
        "intervention/image": "2.4.*",
        "ramsey/uuid": "^4.1.*"
    },

    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "mockery/mockery": "~1.0",
        "sebastian/diff": "^3.0"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Zstore\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Antvel\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },

    "scripts": {
        "test": "./vendor/bin/phpunit"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Zstore\\ZstoreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
